<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" spacing="16.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.attendo.FileController">
   <children>
      <HBox>
         <children>
            <Button fx:id="backButton" mnemonicParsing="false" onAction="#switchToMenu" text="Back" />
         </children>
      </HBox>
      <HBox alignment="CENTER">
         <children>
            <Label fx:id="fileText" text="Label" />
         </children>
      </HBox>
      <HBox alignment="CENTER" spacing="32.0">
         <children>
            <Button fx:id="importButton" minHeight="200.0" minWidth="300.0" mnemonicParsing="false" onAction="#onFileButtonClick" prefHeight="25.0" prefWidth="78.0" text="Select File">
               <font>
                  <Font size="48.0" />
               </font></Button>
         </children>
      </HBox>
   </children>
   <stylesheets>
      <URL value="@secondarymenu.css" />
   </stylesheets>
   <padding>
      <Insets left="32.0" right="32.0" />
   </padding>
</VBox>
