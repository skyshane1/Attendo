<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefWidth="1223.0" spacing="16.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.attendo.ViewController">
   <children>
      <HBox>
         <children>
            <Button fx:id="backButton" mnemonicParsing="false" onAction="#switchToMenu" text="Back" />
         </children>
      </HBox>
      <TableView fx:id="tableview" prefHeight="600.0" prefWidth="432.0">
        <columns>
          <TableColumn fx:id="name" prefWidth="160.0" text="Name" />
          <TableColumn fx:id="banner_id" prefWidth="216.0" text="BannerId" />
          <TableColumn fx:id="swipes" minWidth="3.0" prefWidth="55.0" text="Swipes" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <HBox alignment="CENTER">
         <children>
            <Button mnemonicParsing="false" onAction="#clearDatabase" prefHeight="20.0" prefWidth="1920.0" text="Clear Database" />
         </children>
      </HBox>
   </children>
   <padding>
      <Insets left="32.0" right="32.0" />
   </padding>
   <stylesheets>
           <URL value="@secondarymenu.css" />
      </stylesheets>
</VBox>
